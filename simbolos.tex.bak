\chapter{LISTA DE SÍMBOLOS}

\begin{longtable}{@{}l@{\extracolsep{\fill}} p{4.75in} @{}}
  \textbf{Símbolo} & \textbf{Significado}\\[12pt]
  \endhead
  \hspace{-12pt}{\boldmath $\qed$} & fin de la demostración\\[3pt]
  {\boldmath $\neq$} & distinto\\[3pt]
  {\boldmath $\Leftrightarrow$} & si y sólo si\\[3pt]
  {\boldmath $\varnothing$} & conjunto vacío\\[3pt]
  {\boldmath $E^c$} & complemento de $E$\\[3pt]
  {\boldmath $\varsubsetneq$} & estrictamente contenido\\[3pt]
  {\boldmath $E\setminus F$} & diferencia entre $E$ y $F$\\[3pt]
{\boldmath $\sigma(n)$} & función \emph{sigma}\\[3pt]
{\boldmath $\tau(n)$} & función \emph{tau}\\[3pt]
{\boldmath $I(n)$} & función \emph{identidad}\\[3pt]
{\boldmath $u(n)$} & función \emph{unidad}\\[3pt]
{\boldmath $\nu(n)$} & función \emph{nu}\\[3pt]
{\boldmath $\lambda(n)$} & función \emph{lambda}\\[3pt]
{\boldmath $\mu(n)$} & función \emph{mu}\\[3pt]
{\boldmath $\phi(n)$} & función \emph{phi}, función totiente de Euler\\[3pt]
{\boldmath $(m,n)$} & máximo común divisor entre $m$ y $n$\\[3pt]
{\boldmath $d|n$} & $d$ divisor de $n$\\[3pt]
{\boldmath $(f*g)(n)$} & función convolución de $f$ y $g$\\[3pt]
{\boldmath $I_p(x)$} & función $p$ asociada a la función \emph{Identidad}\\[3pt]
{\boldmath $u_p(x)$} & función $p$ asociada a la función \emph{unidad}\\[3pt]
{\boldmath $\nu_p(x)$} & función $p$ asociada a la función \emph{nu}\\[3pt]
{\boldmath $\lambda_p(x)$} & función $p$ asociada a la función \emph{lambda}\\[3pt]
{\boldmath $\mu_p(x)$} & función $p$ asociada a la función \emph{mu}\\[3pt]
{\boldmath $\phi_p(x)$} & función $p$ asociada a la función \emph{phi}\\[3pt]
{\boldmath $f_p(x)$} & función $p$ asociada a la función aritmética \emph{f}\\[3pt]
{\boldmath $\zeta(s)$} & función zeta de Riemann\\[3pt]
{\boldmath $\sum$} & sumatoria\\[3pt]
{\boldmath $\prod$} & productoria\\[3pt]
{\boldmath $|\cdot|$} & valor absoluto\\[3pt]
{\boldmath $\cancelto{n}{x}$} & cancelación de $x$ al valor $n$\\[3pt]
{\boldmath $\mathbb{R}$} & conjunto de los números reales\\[3pt]
{\boldmath $\mathbb{C}$} & conjunto de los números complejos\\[3pt]
{\boldmath $\mathbb{Z}$} & conjunto de los números enteros\\[3pt]
{\boldmath $\mathbb{Z}^+$} & conjunto de los números enteros positivos\\[3pt]
{\boldmath $\in$} & pertenencia\\[3pt]
{\boldmath $\notin$} & no pertenencia\\[3pt]
